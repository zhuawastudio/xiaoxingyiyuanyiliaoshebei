block\">\r\n        <el-form\r\n                class=\"detail-form-content\"\r\n                ref=\"ruleForm\"\r\n                :model=\"ruleForm\"\r\n                :rules=\"rules\"\r\n                label-width=\"auto\"\r\n                >\r\n            <el-row>\r\n                <el-col :span=\"12\"  v-if=\"sessionTable !='shebei'\">\r\n                    <el-form-item class=\"select\" v-if=\"type!='info'\"  label=\"设备\" prop=\"shebeiId\">\r\n                        <el-select v-model=\"ruleForm.shebeiId\" :disabled=\"ro.shebeiId\" filterable placeholder=\"请选择设备\" @change=\"shebeiChange\">\r\n                            <el-option\r\n                                    v-for=\"(item,index) in shebeiOptions\"\r\n                                    v-bind:key=\"item.id\"\r\n                                    :label=\"item.shebeiName\"\r\n                                    :value=\"item.id\">\r\n                            </el-option>\r\n                        </el-select>\r\n                    </el-form-item>\r\n                </el-col>\r\n\r\n                <el-col :span=\"12\"  v-if=\"sessionTable !='shebei' \">\r\n                    <el-form-item class=\"input\" v-if=\"type!='info'\"  label=\"设备编号\" prop=\"shebeiUuidNumber\">\r\n                        <el-input v-model=\"shebeiForm.shebeiUuidNumber\"\r\n                                  placeholder=\"设备编号\" clearable readonly></el-input>\r\n                    </el-form-item>\r\n                    <div v-else-if=\"type=='info'\">\r\n                        <el-form-item class=\"input\" label=\"设备编号\" prop=\"shebeiUuidNumber\">\r\n                            <el-input v-model=\"ruleForm.shebeiUuidNumber\"\r\n                                      placeholder=\"设备编号\" readonly></el-input>\r\n                        </el-form-item>\r\n                    </div>\r\n                </el-col>\r\n                <el-col :span=\"12\"  v-if=\"sessionTable !='shebei' \">\r\n                    <el-form-item class=\"input\" v-if=\"type!='info'\"  label=\"设备名称\" prop=\"shebeiName\">\r\n                        <el-input v-model=\"shebeiForm.shebeiName\"\r\n                                  placeholder=\"设备名称\" clearable readonly></el-input>\r\n                    </el-form-item>\r\n                    <div v-else-if=\"type=='info'\">\r\n                        <el-form-item class=\"input\" label=\"设备名称\" prop=\"shebeiName\">\r\n                            <el-input v-model=\"ruleForm.shebeiName\"\r\n                                      placeholder=\"设备名称\" readonly></el-input>\r\n                        </el-form-item>\r\n                    </div>\r\n                </el-col>\r\n                <el-col :span=\"12\"  v-if=\"sessionTable !='shebei' \">\r\n                    <el-form-item class=\"input\" v-if=\"type!='info'\"  label=\"设备类型\" prop=\"shebeiValue\">\r\n                        <el-input v-model=\"shebeiForm.shebeiValue\"\r\n                                  placeholder=\"设备类型\" clearable readonly></el-input>\r\n                    </el-form-item>\r\n                    <div v-else-if=\"type=='info'\">\r\n                        <el-form-item class=\"input\" label=\"设备类型\" prop=\"shebeiValue\">\r\n                            <el-input v-model=\"ruleForm.shebeiValue\"\r\n                                      placeholder=\"设备类型\" readonly></el-input>\r\n                        </el-form-item>\r\n                    </div>\r\n                </el-col>\r\n                <el-col :span=\"12\"  v-if=\"sessionTable !='shebei' \">\r\n                    <el-form-item class=\"input\" v-if=\"type!='info'\"  label=\"设备库存\" prop=\"shebeiKucunNumber\">\r\n                        <el-input v-model=\"shebeiForm.shebeiKucunNumber\"\r\n                                  placeholder=\"设备库存\" clearable readonly></el-input>\r\n                    </el-form-item>\r\n                    <div v-else-if=\"type=='info'\">\r\n                        <el-form-item class=\"input\" label=\"设备库存\" prop=\"shebeiKucunNumber\">\r\n                            <el-input v-model=\"ruleForm.shebeiKucunNumber\"\r\n                                      placeholder=\"设备库存\" readonly></el-input>\r\n                        </el-form-item>\r\n                    </div>\r\n                </el-col>\r\n                <el-col :span=\"12\"  v-if=\"sessionTable !='shebei' \">\r\n                    <el-form-item class=\"input\" v-if=\"type!='info'\"  label=\"所在科室\" prop=\"keshiValue\">\r\n                        <el-input v-model=\"shebeiForm.keshiValue\"\r\n                                  placeholder=\"所在科室\" clearable readonly></el-input>\r\n                    </el-form-item>\r\n                    <div v-else-if=\"type=='info'\">\r\n                        <el-form-item class=\"input\" label=\"所在科室\" prop=\"keshiValue\">\r\n                            <el-input v-model=\"ruleForm.keshiValue\"\r\n                                      placeholder=\"所在科室\" readonly></el-input>\r\n                        </el-form-item>\r\n                    </div>\r\n                </el-col>\r\n                <el-col :span=\"12\"  v-if=\"sessionTable !='yonghu'\">\r\n                    <el-form-item class=\"select\" v-if=\"type!='info'\"  label=\"科室职员\" prop=\"yonghuId\">\r\n                        <el-select v-model=\"ruleForm.yonghuId\" :disabled=\"ro.yonghuId\" filterable placeholder=\"请选择科室职员\" @change=\"yonghuChange\">\r\n                            <el-option\r\n                                    v-for=\"(item,index) in yonghuOptions\"\r\n                                    v-bind:key=\"item.id\"\r\n                                    :label=\"item.yonghuName\"\r\n                                    :value=\"item.id\">\r\n                            </el-option>\r\n                        </el-select>\r\n                    </el-form-item>\r\n                </el-col>\r\n\r\n                <el-col :span=\"12\"  v-if=\"sessionTable !='yonghu' \">\r\n                    <el-form-item class=\"input\" v-if=\"type!='info'\"  label=\"职员名称\" prop=\"yonghuName\">\r\n                        <el-input v-model=\"yonghuForm.yonghuName\"\r\n                                  placeholder=\"职员名称\" clearable readonly></el-input>\r\n                    </el-form-item>\r\n                    <div v-else-if=\"type=='info'\">\r\n                        <el-form-item class=\"input\" label=\"职员名称\" prop=\"yonghuName\">\r\n                            <el-input v-model=\"ruleForm.yonghuName\"\r\n                                      placeholder=\"职员名称\" readonly></el-input>\r\n                        </el-form-item>\r\n                    </div>\r\n                </el-col>\r\n                <el-col :span=\"12\"  v-if=\"sessionTable !='yonghu' \">\r\n                    <el-form-item class=\"input\" v-if=\"type!='info'\"  label=\"职员手机号\" prop=\"yonghuPhone\">\r\n                        <el-input v-model=\"yonghuForm.yonghuPhone\"\r\n                                  placeholder=\"职员手机号\" clearable readonly></el-input>\r\n                    </el-form-item>\r\n                    <div v-else-if=\"type=='info'\">\r\n                        <el-form-item class=\"input\" label=\"职员手机号\" prop=\"yonghuPhone\">\r\n                            <el-input v-model=\"ruleForm.yonghuPhone\"\r\n                                      placeholder=\"职员手机号\" readonly></el-input>\r\n                        </el-form-item>\r\n                    </div>\r\n                </el-col>\r\n                <el-col :span=\"12\"  v-if=\"sessionTable !='yonghu' \">\r\n                    <el-form-item class=\"input\" v-if=\"type!='info'\"  label=\"职员身份证号\" prop=\"yonghuIdNumber\">\r\n                        <el-input v-model=\"yonghuForm.yonghuIdNumber\"\r\n                                  placeholder=\"职员身份证号\" clearable readonly></el-input>\r\n                    </el-form-item>\r\n                    <div v-else-if=\"type=='info'\">\r\n                        <el-form-item class=\"input\" label=\"职员身份证号\" prop=\"yonghuIdNumber\">\r\n                            <el-input v-model=\"ruleForm.yonghuIdNumber\"\r\n                                      placeholder=\"职员身份证号\" readonly></el-input>\r\n                        </el-form-item>\r\n                    </div>\r\n                </el-col>\r\n                <el-col :span=\"12\" v-if=\"sessionTable !='yonghu' \">\r\n                    <el-form-item class=\"upload\" v-if=\"type!='info' && !ro.yonghuPhoto\" label=\"职员头像\" prop=\"yonghuPhoto\">\r\n                        <img style=\"margin-right:20px;\" v-bind:key=\"index\" v-for=\"(item,index) in (yonghuForm.yonghuPhoto || '').split(',')\" :src=\"$base.url+item\" width=\"100\" height=\"100\">\r\n                    </el-form-item>\r\n                    <div v-else-if=\"type=='info'\">\r\n                        <el-form-item v-if=\"ruleForm.yonghuPhoto\" label=\"职员头像\" prop=\"yonghuPhoto\">\r\n                            <img style=\"margin-right:20px;\" v-bind:key=\"index\" v-for=\"(item,index) in (ruleForm.yonghuPhoto || '').split(',')\" :src=\"$base.url+item\" width=\"100\" height=\"100\">\r\n                        </el-form-item>\r\n                    </div>\r\n                </el-col>\r\n                <el-col :span=\"12\"  v-if=\"sessionTable !='yonghu' \">\r\n                    <el-form-item class=\"input\" v-if=\"type!='info'\"  label=\"科室\" prop=\"keshiValue\">\r\n                        <el-input v-model=\"yonghuForm.keshiValue\"\r\n                                  placeholder=\"科室\" clearable readonly></el-input>\r\n                    </el-form-item>\r\n                    <div v-else-if=\"type=='info'\">\r\n                        <el-form-item class=\"input\" label=\"科室\" prop=\"keshiValue\">\r\n                            <el-input v-model=\"ruleForm.keshiValue\"\r\n                                      placeholder=\"科室\" readonly></el-input>\r\n                        </el-form-item>\r\n                    </div>\r\n                </el-col>\r\n                <el-col :span=\"12\"  v-if=\"sessionTable !='yonghu' \">\r\n                    <el-form-item class=\"input\" v-if=\"type!='info'\"  label=\"职员邮箱\" prop=\"yonghuEmail\">\r\n                        <el-input v-model=\"yonghuForm.yonghuEmail\"\r\n                                  placeholder=\"职员邮箱\" clearable readonly></el-input>\r\n                    </el-form-item>\r\n                    <div v-else-if=\"type=='info'\">\r\n                        <el-form-item class=\"input\" label=\"职员邮箱\" prop=\"yonghuEmail\">\r\n                            <el-input v-model=\"ruleForm.yonghuEmail\"\r\n                                      placeholder=\"职员邮箱\" readonly></el-input>\r\n                        </el-form-item>\r\n                    </div>\r\n                </el-col>\r\n                <input id=\"updateId\" name=\"id\" type=\"hidden\">\r\n            <input id=\"shebeiId\" name=\"shebeiId\" type=\"hidden\">\r\n            <input id=\"yonghuId\" name=\"yonghuId\" type=\"hidden\">\r\n               <el-col :span=\"12\">\r\n                   <el-form-item class=\"input\" v-if=\"type!='info'\"  label=\"报损数量\" prop=\"shebiebaosunNumber\">\r\n                       <el-input v-model=\"ruleForm.shebiebaosunNumber\"\r\n                                 placeholder=\"报损数量\" clearable  :readonly=\"ro.shebiebaosunNumber\"></el-input>\r\n                   </el-form-item>\r\n                   <div v-else-if=\"type=='info'\">\r\n                       <el-form-item class=\"input\" label=\"报损数量\" prop=\"shebiebaosunNumber\">\r\n                           <el-input v-model=\"ruleForm.shebiebaosunNumber\"\r\n                                     placeholder=\"报损数量\" readonly></el-input>\r\n                       </el-form-item>\r\n                   </div>\r\n               </el-col>\r\n                <el-col :span=\"24\">\r\n                    <el-form-item v-if=\"type!='info'\"  label=\"备注\" prop=\"shebiebaosunText\">\r\n                        <el-input type=\"textarea\"  :readonly=\"ro.shebiebaosunText\" v-model=\"ruleForm.shebiebaosunText\" placeholder=\"备注\"></el-input>\r\n                    </el-form-item>\r\n                    <div v-else-if=\"type=='info'\">\r\n                        <el-form-item v-if=\"ruleForm.shebiebaosunText\" label=\"备注\" prop=\"shebiebaosunText\">\r\n                            <span v-html=\"ruleForm.shebiebaosunText\"></span>\r\n                        </el-form-item>\r\n                    </div>\r\n                </el-col>\r\n            <el-col :span=\"12\" v-if=\"type=='info'\">\r\n                <div>\r\n                    <el-form-item v-if=\"ruleForm.shebiebaosunYesnoTypes\" label=\"申请状态\" prop=\"shebiebaosunYesnoTypes\">\r\n                        <el-input v-model=\"ruleForm.shebiebaosunYesnoValue\" placeholder=\"申请状态\" readonly></el-input>\r\n                    </el-form-item>\r\n                </div>\r\n            </el-col>\r\n            <el-col :span=\"12\" v-if=\"type=='info'\">\r\n                <div>\r\n                    <el-form-item v-if=\"ruleForm.shebiebaosunYesnoText\" label=\"申请结果\" prop=\"shebiebaosunYesnoText\">\r\n                        <span v-html=\"ruleForm.shebiebaosunYesnoText\"></span>\r\n                    </el-form-item>\r\n                </div>\r\n            </el-col>\r\n            </el-row>\r\n            <el-form-item class=\"btn\">\r\n                <el-button v-if=\"type!='info'\" type=\"primary\" class=\"btn-success\" @click=\"onSubmit\">提交</el-button>\r\n                <el-button v-if=\"type!='info'\" class=\"btn-close\" @click=\"back()\">取消</el-button>\r\n                <el-button v-if=\"type=='info'\" class=\"btn-close\" @click=\"back()\">返回</el-button>\r\n            </el-form-item>\r\n        </el-form>\r\n    </div>\r\n</template>\r\n<script>\r\n    import styleJs from \"../../../utils/style.js\";\r\n    // 数字，邮件，手机，url，身份证校验\r\n    import { isNumber,isIntNumer,isEmail,isPhone, isMobile,isURL,checkIdCard } from \"@/utils/validate\";\r\n    import utilsJs, {getYearFormat,getMonthFormat,getDateFormat,getDatetimeFormat} from \"../../../utils/utils.js\";\r\n    export default {\r\n        data() {\r\n            return {\r\n                addEditForm:null,\r\n                id: '',\r\n                type: '',\r\n                sessionTable : \"\",//登录账户所在表名\r\n                role : \"\",//权限\r\n                userId:\"\",//当前登录人的id\r\n                shebeiForm: {},\r\n                yonghuForm: {},\r\n                ro:{\r\n                    shebeiId: false,\r\n                    yonghuId: false,\r\n                    shebiebaosunNumber: false,\r\n                    shebiebaosunText: false,\r\n                    shebiebaosunYesnoTypes: false,\r\n                    shebiebaosunYesnoText: false,\r\n                    insertTime: false,\r\n                },\r\n                ruleForm: {\r\n                    shebeiId: '',\r\n                    yonghuId: '',\r\n                    shebiebaosunNumber: '',\r\n                    shebiebaosunText: '',\r\n                    shebiebaosunYesnoTypes: '',\r\n                    shebiebaosunYesnoText: '',\r\n                    insertTime: '',\r\n                },\r\n                shebiebaosunYesnoTypesOptions : [],\r\n                shebeiOptions : [],\r\n                yonghuOptions : [],\r\n                rules: {\r\n                   shebeiId: [\r\n                              { required: true, message: '设备不能为空', trigger: 'blur' },\r\n                              {  pattern: /^[0-9]*$/,\r\n                                  message: '只允许输入整数',\r\n                                  trigger: 'blur'\r\n                              }\r\n                          ],\r\n                   yonghuId: [\r\n                              { required: true, message: '科室职员不能为空', trigger: 'blur' },\r\n                              {  pattern: /^[0-9]*$/,\r\n                                  message: '只允许输入整数',\r\n                                  trigger: 'blur'\r\n                              }\r\n                          ],\r\n                   shebiebaosunNumber: [\r\n                              { required: true, message: '报损数量不能为空', trigger: 'blur' },\r\n                              {  pattern: /^[0-9]*$/,\r\n                                  message: '只允许输入整数',\r\n                                  trigger: 'blur'\r\n                              }\r\n                          ],\r\n                   shebiebaosunText: [\r\n                              { required: true, message: '备注不能为空', trigger: 'blur' },\r\n                          ],\r\n                   shebiebaosunYesnoTypes: [\r\n                              { required: true, message: '申请状态不能为空', trigger: 'blur' },\r\n                              {  pattern: /^[0-9]*$/,\r\n                                  message: '只允许输入整数',\r\n                                  trigger: 'blur'\r\n                              }\r\n                          ],\r\n                   shebiebaosunYesnoText: [\r\n                              { required: true, message: '申请结果不能为空', trigger: 'blur' },\r\n                          ],\r\n                   insertTime: [\r\n                              { required: true, message: '录入时间不能为空', trigger: 'blur' },\r\n                          ],\r\n                }\r\n            };\r\n        },\r\n        props: [\"parent\"],\r\n        computed: {\r\n        },\r\n        created() {\r\n            //获取当前登录用户的信息\r\n            this.sessionTable = this.$storage.get(\"sessionTable\");\r\n            this.role = this.$storage.get(\"role\");\r\n            this.userId = this.$storage.get(\"userId\");\r\n\r\n\r\n            if (this.role != \"管理员\"){\r\n            }\r\n            this.addEditForm = styleJs.addStyle();\r\n            this.addEditStyleChange()\r\n            this.addEditUploadStyleChange()\r\n            //获取下拉框信息\r\n                this.$http({\r\n                    url:`dictionary/page?page=1&limit=100&sort=&order=&dicCode=shebiebaosun_yesno_types`,\r\n                    method: \"get\"\r\n                }).then(({ data }) => {\r\n                    if (data && data.code === 0) {\r\n                        this.shebiebaosunYesnoTypesOptions = data.data.list;\r\n                    }\r\n                });\r\n\r\n         this.$http({\r\n             url: `shebei/page?page=1&limit=100`,\r\n             method: \"get\"\r\n         }).then(({ data }) => {\r\n             if (data && data.code === 0) {\r\n                this.shebeiOptions = data.data.list;\r\n            }\r\n         });\r\n         this.$http({\r\n             url: `yonghu/page?page=1&limit=100`,\r\n             method: \"get\"\r\n         }).then(({ data }) => {\r\n             if (data && data.code === 0) {\r\n                this.yonghuOptions = data.data.list;\r\n            }\r\n         });\r\n\r\n        },\r\n        mounted() {\r\n        },\r\n        methods: {\r\n            // 下载\r\n            download(file){\r\n                window.open(`${file}`)\r\n            },\r\n            // 初始化\r\n            init(id,type) {\r\n                if (id) {\r\n                    this.id = id;\r\n                    this.type = type;\r\n                }\r\n                if(this.type=='info'||this.type=='else'){\r\n                    this.info(id);\r\n                }\r\n                // 获取用户信息\r\n                this.$http({\r\n                    url:`${this.$storage.get(\"sessionTable\")}/session`,\r\n                    method: \"get\"\r\n                }).then(({ data }) => {\r\n                    if (data && data.code === 0) {\r\n                        var json = data.data;\r\n                    } else {\r\n                        this.$message.error(data.msg);\r\n                    }\r\n                });\r\n            },\r\n            shebeiChange(id){\r\n                this.$http({\r\n                    url: `shebei